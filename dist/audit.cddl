LogEntry = EventGroup

EventGroup = {
  context: ContextId
  origin: BuildID
  start: time
  end: time
  events: [+ Event]
}

Event = NewContext / Data

ContextId = bstr .size 16
BuildID = bstr .size (20..64)

NewContext = {
  NewContext: {
    parent: ContextId
    origin: BuildID
  }
}

Data = {
  Data: {
    key: tstr
    value: uint .size 8 / tstr / bstr
  }
}
